{
	"name": "Vimium C",
	"@time": "8/4/2025, 11:44:42 AM",
	"time": 1754325882026,
	"environment": {
		"extension": "2.12.2",
		"platform": "mac",
		"chromium": 138
	},
	"allBrowserUrls": true,
	"exclusionRules": [],
	"filterLinkHints": true,
	"keyLayout": 2,
	"keyMappings": [
		"#!no-check",
		"# Search",
		"# map <space> LinkHints.activate",
		"# map <s-space> LinkHints.activateOpenInNewTab",
		"map s LinkHints.activate",
		"map S LinkHints.activateOpenInNewTab",
		"map d removeTab",
		"map o createTab",
		"map e LinkHints.activateEdit",
		"map t LinkHints.activateFocus",
		"",
		"# Misc",
		"# map r toggleReaderMode",
		"",
		"# Tab Navigation",
		"map <s-f1> previousTab",
		"map <s-f12> nextTab",
		"",
		"# ===== Page Scrolling =====",
		"map <s-f4> scrollUp",
		"map <s-f6> scrollDown",
		"# map <pageup> scrollFullPageUp",
		"# map <pagedown> scrollFullPageDown",
		"",
		"# ===== Quick Open =====",
		"map <s-f3> Vomnibar.activate",
		"",
		"# Buffer/Tab Management (VS Code 'b' prefix shortcuts)",
		"unmap b                     # unmap default bookmark shortcuts to repurpose 'b'",
		"unmap B",
		"map bd removeTab            # b d -> Close current tab",
		"map bD restoreTab",
		"map bx closeTabsOnLeft",
		"map bX closeTabsOnRight",
		"map bo closeOtherTabs       # b o -> Close other tabs (keep current; pinned tabs are preserved)",
		"map bP closeOtherTabs       # b P -> Close all non-pinned tabs (same as above, leaves pinned tabs)",
		"map bp togglePinTab         # b p -> Pin current tab",
		"map bu togglePinTab         # b u -> Unpin current tab",
		"map bb visitPreviousTab     # b b -> Switch to last active tab (previously visited tab)",
		"map bh moveTabLeft          # b h -> Move tab left (reorder tab)",
		"map bl moveTabRight         # b l -> Move tab right (reorder tab)",
		"map bn createTab         # b l -> Move tab right (reorder tab)",
		"map bm toggleMuteTab",
		"map bs Vomnibar.activateTabs",
		"map fl copyCurrentUrl",
		"",
		"# UI",
		"map ui zoomIn",
		"map uo zoomOut",
		"map us zoomReset",
		"",
		"map ]b nextTab              # ] b -> Next tab (cycle forward)",
		"map [b previousTab          # [ b -> Previous tab (cycle backward)",
		""
	],
	"linkHintCharacters": "tsraneiodhgmplfuc,bjvk",
	"linkHintNumbers": "1234567890",
	"nextPatterns": "next,more,newer,>,›,→,»,≫,>>",
	"previousPatterns": "prev,previous,back,older,<,‹,←,«,≪,<<",
	"searchEngines": [
		"g|go|gg|google|Google: https://www.google.com/search?q=%s \\",
		"  www.google.com re=/^(?:\\.[a-z]{2,4})?\\/search\\b.*?[#&?]q=([^#&]*)/i \\",
		"  blank=https://www.google.com/ Google",
		"br|brave: https://search.brave.com/search?q=%s Brave",
		"d|dd|ddg|duckduckgo: https://duckduckgo.com/?q=%s DuckDuckGo",
		"g.m|gm|g.map|gmap: https://www.google.com/maps?q=%s \\",
		"  blank=https://www.google.com/maps Google Maps",
		"y|yt: https://www.youtube.com/results?search_query=%s \\",
		"  blank=https://www.youtube.com/ YouTube",
		"w|wiki: https://www.wikipedia.org/w/index.php?search=%s Wikipedia",
		"g.s|gs|gscholar: https://scholar.google.com/scholar?q=$s \\",
		"  scholar.google.com re=/^(?:\\.[a-z]{2,4})?\\/scholar\\b.*?[#&?]q=([^#&]*)/i \\",
		"  blank=https://scholar.google.com/ Google Scholar",
		"",
		"az|amazon: https://www.amazon.com/s?k=%s \\",
		"  blank=https://www.amazon.com/ Amazon",
		"",
		"\\:i: vimium://sed/s/^//,lower\\ $S re= Lower case",
		"v.m|math: vimium://math\\ $S re= Calculate",
		"v.p: vimium://parse\\ $S re= Redo Search",
		"gh|github: https://github.com/search?q=$s \\",
		"  blank=https://github.com/ GitHub Repo",
		"js\\:|Js: javascript:\\ $S; JavaScript",
		""
	],
	"smoothScroll": false,
	"vimSync": true,
	"waitForEnter": false
}
