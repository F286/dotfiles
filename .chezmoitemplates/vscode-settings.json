{
  // Cursor behavior and line numbers
  "editor.cursorSurroundingLines": 8,
  "editor.cursorSurroundingLinesStyle": "all",
  "editor.cursorBlinking": "solid",
  "editor.cursorSmoothCaretAnimation": "off",
  "editor.cursorStyle": "line",
  "editor.fontSize": 12,
  "editor.fontFamily": "'JetBrains Mono', 'JetBrainsMono Nerd Font', monospace",
  "editor.fontLigatures": true,
  "editor.fontWeight": "250",
  "editor.fontVariations": "'wght' 250",
  "editor.letterSpacing": 0.35,
  "editor.minimap.enabled": false,
  "debug.inlineValues":"on",
  "chat.agent.enabled": true,
  "chat.tools.autoApprove": true,
  "chat.agent.maxRequests": 50,
  "terminal.integrated.letterSpacing": 1.0,
  "terminal.integrated.fontWeight": "normal",
  "terminal.integrated.fontFamily": "'JetBrainsMono Nerd Font', monospace",
  "terminal.integrated.minimumContrastRatio": 1,
  "terminal.integrated.profiles.osx": {
    "Find Files": {
      "path": "/bin/zsh",
      "args": [
        "-lc",
        // Ensure brew bins are visible; source theme; feed ripgrep config; run ff
        "export PATH=/opt/homebrew/bin:/usr/local/bin:$PATH; source ~/.config/fzf/tokyonight.zsh 2>/dev/null; export RIPGREP_CONFIG_PATH=~/.ripgreprc; ~/.local/bin/ff || true; exit 0"
      ],
      "icon": "search",
      "overrideName": true
    },
    "Search Grep": {
      "path": "/bin/zsh",
      "args": [
        "-lc",
          "export PATH=/opt/homebrew/bin:/usr/local/bin:$PATH; source ~/.config/fzf/tokyonight.zsh 2>/dev/null; export RIPGREP_CONFIG_PATH=~/.ripgreprc; ~/.local/bin/sg || true; exit 0"
      ],
      "icon": "search",
      "overrideName": true
    },
    "Fast Grep": {
      "path": "/bin/zsh",
      "args": [
        "-lc",
          "export PATH=/opt/homebrew/bin:/usr/local/bin:$PATH; source ~/.config/fzf/tokyonight.zsh 2>/dev/null; export RIPGREP_CONFIG_PATH=~/.ripgreprc; ~/.local/bin/sf || true; exit 0"
      ],
      "icon": "search",
      "overrideName": true
    },
    "Search Text": {
      "path": "/bin/zsh",
      "args": [
        "-lc",
          "export PATH=/opt/homebrew/bin:/usr/local/bin:$PATH; source ~/.config/fzf/tokyonight.zsh 2>/dev/null; export RIPGREP_CONFIG_PATH=~/.ripgreprc; ~/.local/bin/st --code || true; exit 0"
      ],
      "icon": "search",
      "overrideName": true
    },
    "Search Index": {
      "path": "/bin/zsh",
      "args": [
        "-lc",
          "export PATH=/opt/homebrew/bin:/usr/local/bin:$PATH; source ~/.config/fzf/tokyonight.zsh 2>/dev/null; export RIPGREP_CONFIG_PATH=~/.ripgreprc; ~/.local/bin/si || true; exit 0"
      ],
      "icon": "search",
      "overrideName": true
    },
    "Lazy Git": {
      "path": "/bin/zsh",
      "args": [
        "-lc",
        "export PATH=/opt/homebrew/bin:/usr/local/bin:$PATH; source ~/.config/fzf/tokyonight.zsh 2>/dev/null; lazygit || true; exit 0"
      ],
      "icon": "git-branch",
      "overrideName": true
    }
  },
   
  "vim.timeout": 1000000,
  // You already have these, good for overall speed
  "terminal.integrated.enablePersistentSessions": true,
  "terminal.integrated.persistentSessionReviveProcess": "onExit",
  "terminal.integrated.persistentSessionScrollback": 10000,
  "terminal.integrated.sendKeybindingsToShell": true,
  "terminal.integrated.allowChords": true,
  "terminal.integrated.confirmOnExit": "never",
  "terminal.integrated.defaultLocation": "view",
  "terminal.integrated.focusAfterRun": "accessible-buffer",
  "terminal.integrated.hideOnLastClosed": true,
  "terminal.integrated.hideOnStartup": true,
  "terminal.integrated.showExitAlert": false,
  "terminal.integrated.commandsToSkipShell": [
    "workbench.action.focusActiveEditorGroup",
    "workbench.action.terminal.focusPreviousPane",
    "workbench.action.terminal.focusNextPane",
    "workbench.action.togglePanel",
    "workbench.action.terminal.sendSequence"
  ],
  "editor.lineNumbers": "relative",
  "editor.renderLineHighlight": "all",
  "workbench.colorTheme": "Tokyo Night Storm",
  "workbench.preferredDarkColorTheme": "Tokyo Night Storm",
  "workbench.colorCustomizations": {
    "editorCursor.foreground": "#D0CCB2",
    "editorCursor.background": "#000000",
    "editor.lineHighlightBackground": "#2F334D",
    "editorLineNumber.activeForeground": "#FF9E3B",
    "editor.selectionBackground": "#313F72",
    "editor.background": "#222436",
    "editorSuggestWidget.selectedBackground": "#2F334D",
    "editorSuggestWidget.selectedForeground": "#FFFFFF",
    "editorSuggestWidget.selectedIconForeground": "#FFFFFF"
  },
  "workbench.quickOpen.centered": true,
  "window.commandCenter": false,
  "search.location": "panel",
  // Disable AI code suggestions and built-in tag closing
  "editor.inlineSuggest.enabled": false,
  "github.copilot.enable": {
    "*": false
  },
  "html.autoClosingTags": false,
  "auto-close-tag.activationOnLanguage": [
    "html",
    "css",
    "xml",
    "php",
    "blade",
    "ejs",
    "jinja",
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "plaintext",
    "markdown",
    "vue",
    "liquid",
    "erb",
    "lang-cfml",
    "cfml",
    "HTML (Eex)"
],
"editor.autoClosingQuotes":"never",
"editor.autoClosingBrackets": "languageDefined",
"editor.autoClosingPairs": "never",
"editor.autoSurround": "never",
"html.autoCreateQuotes": false,
  // Vim extension settings
  "vim.useSystemClipboard": true,
  "vim.useCtrlKeys": true,
  "vim.surround": true,
  "vim.leader": "<space>",
  "vim.hlsearch": true,
  "vim.highlightedyank.enable": true,
  "vim.highlightedyank.color": "rgba(255, 223, 93, 0.3)", // warm gold, soft opacity
  "vim.highlightedyank.duration": 290, // milliseconds
  "vim.handleKeys": {
    "<tab>": false,
    "<S-tab>": false,
    "<S-j>": false,
    "<S-k>": false,
    "<C-o>": true,
    "<C-i>": true,
    "<C-c>": false,
    "<C-v>": true,
    "<C-x>": false,
    "<C-a>": false,
    "<D-c>": false,
    "<D-v>": true,
    "<D-x>": false,
    "<D-a>": false,
    "<D-o>": true,
    "<D-i>": true
  },
  "vim.cursorStylePerMode": {
    "normal": "block",
    "insert": "line",
    "replace": "underline",
    "visual": "line-thin",
    "visualLine": "line-thin",
    "visualBlock": "line-thin"
  },
  "vscode-textobjects.vimKeybindings": {
    "select": "v",
    "change": "c",
    "delete": "d",
    "yank": "y",
    "go to next start": "[",
    "go to next end": "[ [",
    "go to previous start": "]",
    "go to previous end": "] ]",
    "inner": "i",
    "outer": "a",
    "array": "a",
    "call": "m",
    "class": "k",
    "comment": "c",
    "conditional": "i",
    "function": "f",
    "lhs": "h",
    "loop": "l",
    "node": "n",
    "object": "o",
    "rhs": "r",
    "parameters": "e",
    "string": "q",
    "type": "y",
    "variable": "v"
  },
  "vim.operatorPendingModeKeyBindingsNonRecursive": [
    // Entire buffer text object
    { "before": ["a", "g"], "after": ["a", "e"] },
    { "before": ["i", "g"], "after": ["i", "e"] }
  ],
  "vim.visualModeKeyBindingsNonRecursive": [
    // Navigate by visual lines
    {
      "before": [
        "j"
      ],
      "after": [
        "g",
        "j"
      ]
    },
    {
      "before": [
        "k"
      ],
      "after": [
        "g",
        "k"
      ]
    },
    {
      "before": [
        "<leader>",
        "c",
        "a"
      ],
      "commands": [
        "editor.action.quickFix"
      ]
    },
    {
      "before": [
        "<leader>",
        "c",
        "f"
      ],
      "commands": [
        "editor.action.formatSelection"
      ]
    },
    // Entire buffer text object
    { "before": ["a", "g"], "after": ["a", "e"] },
    { "before": ["i", "g"], "after": ["i", "e"] }
  ],
  "vim.insertModeKeyBindingsNonRecursive": [
    {
      "before": ["<C-v>"],
      "commands": ["editor.action.clipboardPasteAction"]
    },
    {
      "before": ["<D-v>"],
      "commands": ["editor.action.clipboardPasteAction"]
    },

    {
      "before": [
        "<C-k>"
      ],
      "commands": [
        "editor.action.triggerParameterHints"
      ]
    },
    {
      "before": [
        "<A-j>"
